{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "dev": "cross-env NODE_ENV=development ts-node-dev src/index.ts",
    "dev-pg:up": "docker compose -f docker-compose.dev.yml up",
    "dev-pg:down": "docker compose -f docker-compose.dev.yml down",
    "dev-pg:psql": "docker exec -it cicd_pg_dev_db psql --username=postgres --dbname=cicd_dev",
    "dev-pg:umzug": "cross-env NODE_ENV=development node src/util/umzug/index.js",
    "start": "cross-env NODE_ENV=production node build/index.js",
    "start:test": "cross-env NODE_ENV=test node build/index.js",
    "test": "cross-env NODE_ENV=test jest --verbose --runInBand",
    "test-pg:up": "docker compose -f docker-compose.test.yml up -d",
    "test-pg:down": "docker compose -f docker-compose.test.yml down",
    "lint": "eslint ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "pg": "^8.11.3",
    "sequelize": "^6.36.0",
    "umzug": "^3.7.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin-js": "^1.6.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.20",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "clearMocks": true,
    "setupFilesAfterEnv": [
      "./test/suiteSetup.js"
    ]
  }
}

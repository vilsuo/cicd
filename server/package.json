{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon src/index.js",
    "dev-pg:up": "docker compose -f docker-compose.dev.yml up",
    "dev-pg:down": "docker compose -f docker-compose.dev.yml down",
    "start": "cross-env NODE_ENV=production node src/index.js",
    "start:test": "cross-env NODE_ENV=test node src/index.js",
    "test": "cross-env NODE_ENV=test jest --verbose --runInBand",
    "test-pg:up": "docker compose -f docker-compose.test.yml up -d",
    "test-pg:down": "docker compose -f docker-compose.test.yml down",
    "lint": "eslint ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.2",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "pg": "^8.11.3",
    "sequelize": "^6.36.0",
    "umzug": "^3.7.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin-js": "^1.6.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "supertest": "^6.3.4"
  },
  "jest": {
    "clearMocks": true,
    "setupFilesAfterEnv": [
      "./test/suiteSetup.js"
    ]
  }
